#include "holberton.h"
/**
 * print_number - display numbers function
 *
 * @n: integer
 */
void print_number(int n)
{
	if (n > 1000)
	{
		n1 = n / 1000;
		n2 = (n % 1000) / 100;
		n3 = (n / 10) % 10;
		n4 = n % 10;
		_putchar(n1 + '0');
		_putchar(n2 + '0');
		_putchar(n3 + '0');
		_putchar(n4 + '0');
	}
}
