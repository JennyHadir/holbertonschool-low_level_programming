#include "holberton.h"
/**
 * jack_bauer - time function
 *
 * Return:
 */
void jack_bauer(void)
{
	int i, hr, mn;

	hr = 00;
	mn = 00;
	for (i = 0; i <= 60; i++)
	{
		if (i == 60)
		{
			hr = hr + 1;
		}
		else
		{
			mn = mn + 1;
		}
		_putchar(hr + '0');
		_putchar(':');
		_putchar(mn + '0');
		_putchar('\n');
	}
}
