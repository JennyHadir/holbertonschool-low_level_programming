#include "holberton.h"
/**
 * rev_string - reverse a string function
 *
 * @s: string
 * Return: void
 */
void rev_string(char *s)
{
	int i, j, len;
	char str;

	for (len = 0; s[len] != '\0'; len++)
	{
		len++;
	}
	j = len - 1;
	while (j > i)
	{
		str = s[i];
		s[i] = s[j];
		s[j] = str;
		j--;
		i++;
	}
}
